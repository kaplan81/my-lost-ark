{
  "name": "kaplan81-ng2-aot-wp2",
  "version": "1.0.0",
  "description": "An Angular 2 AOT starter with Webpack 2 configuration",
  "scripts": {
    "start": "webpack-dev-server --inline --watch",
    "start:jit": "npm run build && npm run serve",
    "start:aot": "npm run build:aot && npm run clean:unbundled && npm run serve:aot",
    "build": "npm run clean && webpack --config config/webpack.dev.js --progress --profile --bail",
    "build:aot": "npm run precompile && webpack --config config/webpack.prod.js --progress --profile --bail",
    "precompile": "npm run clean:aot && ngc -p tsconfig-aot.json --locale=en-US",
    "clean": "rimraf dist",
    "clean:aot": "rimraf aot dist-aot",
    "clean:unbundled": "rimraf dist-aot/unbundled-aot",
    "serve": "http-server ./dist -o",
    "serve:aot": "http-server ./dist-aot -p 7070 -o"
  },
  "keywords": [],
  "author": "Andres Gesteira @kaplan81",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "2.1.0",
    "@angular/compiler": "2.1.0",
    "@angular/compiler-cli": "2.1.0",
    "@angular/core": "2.1.0",
    "@angular/forms": "2.1.0",
    "@angular/http": "2.1.0",
    "@angular/platform-browser": "2.1.0",
    "@angular/platform-browser-dynamic": "2.1.0",
    "@angular/platform-server": "2.1.0",
    "@angular/router": "3.1.0",
    "angular2-in-memory-web-api": "0.0.21",
    "animate.css": "^3.5.2",
    "bootstrap": "^4.0.0-alpha.5",
    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "^0.6.26"
  },
  "devDependencies": {
    "@types/core-js": "^0.9.34",
    "@types/jasmine": "^2.5.35",
    "@types/node": "^6.0.45",
    "@types/protractor": "^1.5.20",
    "@types/selenium-webdriver": "2.44.29",
    "angular2-template-loader": "^0.4.0",
    "angular2-webpack2-lazy-children-loader": "^1.0.1",
    "awesome-typescript-loader": "^2.2.4",
    "canonical-path": "0.0.2",
    "concurrently": "^3.1.0",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.15.0",
    "http-server": "^0.9.0",
    "istanbul-instrumenter-loader": "^0.2.0",
    "jasmine-core": "~2.5.2",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-coverage": "^1.1.1",
